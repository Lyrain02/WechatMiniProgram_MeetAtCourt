/**app.wxss**/
page{
  color: #2a5389;
  font-size: 36rpx;
  letter-spacing:5rpx;
}

.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 

.background {
  width: 100%;
  height: 100%;
  position:fixed; 
  background-size:100% 100%;
  z-index: -1;
}

.navigator{
  display: flex;
  flex-direction: row;
  justify-content: flex-start;
  padding: 20rpx;
}

.icon{
  width: 50rpx;
  height: 50rpx;
}